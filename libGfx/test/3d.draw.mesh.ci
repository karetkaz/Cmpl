gxMesh teapot = gxMesh.openObj("asset/mesh/teapot.obj");
gxMesh bunny = gxMesh.openObj("asset/mesh/bunny.obj");

float32 center[3]; center[0] = center[1] = center[2] = 0;
float32 resize[3]; resize[0] = resize[1] = resize[2] = 1.5f;

teapot.normalize(0, center, resize);
bunny.normalize(0, center, resize);

debug("Mesh.bunny", bunny);
debug("Mesh.teapot", teapot);

teapot.showMesh(1440, 900, 20);
bunny.showMesh(1440, 900, 20);

teapot.destroy();
bunny.destroy();
