
struct vec4f: 0 {
	float32 data[4];
	struct {
		float32 x;
		float32 y;
		float32 z;
		float32 w;
	}
}

vec4f vec4f(float32 x, float32 y, float32 z, float32 w) {
	return { x: x, y: y, z: z, w: w };
}
vec4f vec4f(float32 x, float32 y, float32 z) {
	return { x: x, y: y, z: z, w: 0 };
}
vec4f vec4f(float32 xyzw) {
	return { x: xyzw, y: xyzw, z: xyzw, w: xyzw };
}
