// lookup test
inline int_ = int(1990);
inline int_() = int(1995);
inline int_(int32 _) = int(2010);
inline int_(float64 _) = int(2015);

int32 _int1 = int_;			// 1990
int32 _int2 = int_();		// 1995
int32 _int3 = int_(0);		// 2010
int32 _int4 = int_(0.);		// 2015

assert(_int1 == 1990);
assert(_int2 == 1995);
assert(_int3 == 2010);
assert(_int4 == 2015);

assert(int_ == 1990);
assert(int_() == 1995);
assert(int_(0) == 2010);
assert(int_(0.) == 2015);

