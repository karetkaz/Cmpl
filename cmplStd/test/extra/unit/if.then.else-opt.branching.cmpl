#!/usr/bin/env -S ${CMPL_HOME}/extras/CmplDiffTest.sh 33

int test(uint64 x) {
	if (x == 0) {
		return -1;
	}

	if (x == 0) else {
		return -2;
	}

	if (x == 0) {
		return -3;
	} else {
		return -4;
	}

	return x;
}

