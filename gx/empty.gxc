void mouseCB(int btn, int x, int y) {
	static int oldbtn = 0;
	if (btn != oldbtn) {
		print("mouseHnd(");
		print("btn = %08x", btn);
		print(", x = %d", x);
		print(", y = %d", y);
		print(")\n");
		oldbtn = btn;
	}
}
Gui.setMouseHandler(mouseCB);
