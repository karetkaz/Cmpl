// iterating ...struct Fibonacci {	int curr;	int next;	const int max;}define Fibonacci(int max) = Fibonacci(0, 1, max);// struct is self iterable.define iterator(Fibonacci fib) = (fib);// iterator implementation.bool next(Fibonacci &fib, int32 &value) {	if (fib.curr < fib.max) {		value = fib.curr;		int next = fib.curr + fib.next;		fib.curr = fib.next;		fib.next = next;		return true;	}	return false;}for (int32 value: Fibonacci(2000000000)) {	print("Fibonacci: %d\n", value);}