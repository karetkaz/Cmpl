// iterating ...struct Fibonacci {	int64 curr;	int64 next;	const int64 max;}define Fibonacci(int64 max) = Fibonacci(0, 1, max);// struct is self iterable.define iterator(Fibonacci fib) = (fib);// iterator implementation.bool next(Fibonacci &fib, int64 &value) {	if (fib.curr < fib.max) {		value = fib.curr;		int64 next = fib.curr + fib.next;		fib.curr = fib.next;		fib.next = next;		return true;	}	return false;}//~ for (int64 value: Fibonacci(200000000000000)) {	//~ print("Fibonacci: %U\n", value);//~ }