// iterating ...struct Fibonacci {	int64 a;	int64 b;	const int64 max;}inline Fibonacci(int64 max) = Fibonacci(0, 1, max);// struct is self iterable.inline iterator(Fibonacci fib) = (fib);// iterator implementation.bool next(Fibonacci &fib, int64 &value) {	if (fib.a < fib.max) {		value = fib.a;		int64 next = fib.a + fib.b;		fib.a = fib.b;		fib.b = next;		return true;	}	return false;}for (int64 value : Fibonacci(200000000000000)) {	debug("Fib", variant(&value));}